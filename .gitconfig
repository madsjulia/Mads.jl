[core]
	bare = false
	repositoryformatversion = 0
	filemode = true
	ignorecase = true
	precomposeunicode = true
	logallrefupdates = true
[remote "gl"]
	url = https://gitlab.com/mads/Mads.jl.git
	fetch = +refs/heads/*:refs/remotes/origin/*
	pushurl = git@gitlab.com:mads/Mads.jl.git
[remote "ghm"]
    url = https://github.com/montyvesselinov/Mads.jl.git
    fetch = +refs/heads/*:refs/remotes/origin/*
    pushurl = git@github.com:montyvesselinov/Mads.jl.git
[remote "origin"]
	url = https://github.com/madsjulia/Mads.jl.git
	fetch = +refs/heads/*:refs/remotes/origin/*
	pushurl = git@github.com:madsjulia/Mads.jl.git
[branch "master"]
	remote = origin
	merge = refs/heads/master
